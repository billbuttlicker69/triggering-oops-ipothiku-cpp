#include <iostream>
using namespace std;
int main()
{
    string s = "clow";
    try
    {
     if(s == "clown")
     {
         throw(s);
         cout<<"after throw/n";
     }
     else
     {
         cout<<"throw..";
     }
    }
     catch(string s)
     {
         cout<<"caught"<<s;
     }
        
   return 0;
}
